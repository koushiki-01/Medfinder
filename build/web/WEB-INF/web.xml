<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TimeServe</servlet-name>
        <servlet-class>webpack.TimeServe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrintForm</servlet-name>
        <servlet-class>webpack.PrintForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>webpack.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorkRegister</servlet-name>
        <servlet-class>webpack.WorkRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedInsert</servlet-name>
        <servlet-class>webpack.MedInsert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TimeServe</servlet-name>
        <url-pattern>/TimeServe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrintForm</servlet-name>
        <url-pattern>/PrintForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorkRegister</servlet-name>
        <url-pattern>/WorkRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedInsert</servlet-name>
        <url-pattern>/MedInsert</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
